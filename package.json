{
  "name": "bazar-core",
  "version": "0.1.0",
  "description": "Lisk-SDK Application",
  "author": "Diego Cortes",
  "license": "Apache License 2.0",
  "keywords": [
    "blockchain",
    "lisk",
    "nodejs",
    "javascript",
    "typescript"
  ],
  "homepage": "",
  "repository": {},
  "engines": {
    "node": "16"
  },
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "eslint --fix --ext .js,.ts .",
    "format": "prettier --write '**/*'",
    "prepack": "oclif-dev manifest && oclif-dev readme --multi --dir=docs/commands && npm shrinkwrap && npm prune --production && npm shrinkwrap",
    "prebuild": "if test -d dist; then rm -r dist; fi; rm -f tsconfig.tsbuildinfo; rm -f npm-shrinkwrap.json",
    "start": "echo Run \"./bin/run start\" to start the app",
    "build": "tsc",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --passWithNoTests --coverage=true --coverage-reporters=text",
    "test:ci": "jest --passWithNoTests --coverage=true --coverage-reporters=json --verbose",
    "version": "oclif-dev readme --multi --dir=docs/commands && git add README.md docs",
    "prepublishOnly": "npm ci && npm run lint && npm run build"
  },
  "bin": {
    "bazar-core": "./bin/run"
  },
  "lisk": {
    "addressPrefix": "lsk"
  },
  "oclif": {
    "bin": "bazar-core",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help"
    ],
    "topics": {
      "account": {
        "description": "Commands relating to bazar-core accounts."
      },
      "block": {
        "description": "Commands relating to bazar-core blocks."
      },
      "blockchain": {
        "description": "Commands relating to bazar-core blockchain data."
      },
      "console": {
        "description": "bazar-core interactive REPL session to run commands."
      },
      "config": {
        "description": "Commands relating to bazar-core node configuration."
      },
      "forger-info": {
        "description": "Commands relating to bazar-core forger-info data."
      },
      "forging": {
        "description": "Commands relating to bazar-core forging."
      },
      "hash-onion": {
        "description": "Create hash onions to be used by the forger."
      },
      "node": {
        "description": "Commands relating to bazar-core node."
      },
      "passphrase": {
        "description": "Commands relating to bazar-core passphrases."
      },
      "sdk": {
        "description": "Commands relating to Lisk SDK development."
      },
      "transaction": {
        "description": "Commands relating to bazar-core transactions."
      }
    }
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/dist",
    "/config",
    "/docs"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@liskhq/lisk-framework-dashboard-plugin": "0.1.7",
    "@liskhq/lisk-framework-faucet-plugin": "0.1.7",
    "@oclif/command": "1.8.29",
    "@oclif/plugin-autocomplete": "2.3.1",
    "@oclif/plugin-help": "5.2.10",
    "axios": "1.4.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fs-extra": "11.1.1",
    "husky": "^8.0.3",
    "inquirer": "9.2.7",
    "ipfs-http-client": "57.0.3",
    "lisk-commander": "5.1.10",
    "lisk-sdk": "5.2.2",
    "multer": "^1.4.5-lts.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "tar": "6.1.15",
    "tslib": "2.6.0"
  },
  "devDependencies": {
    "@oclif/config": "1.18.10",
    "@oclif/dev-cli": "1.26.10",
    "@types/fs-extra": "11.0.1",
    "@types/jest": "29.5.2",
    "@types/jest-when": "3.5.2",
    "@types/multer": "1.4.7",
    "@types/node": "16.18.37",
    "@types/tar": "6.1.5",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "eslint": "8.43.0",
    "eslint-config-lisk-base": "2.0.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.2",
    "globby": "13.2.0",
    "jest": "29.5.0",
    "jest-extended": "4.0.0",
    "jest-when": "3.5.2",
    "lint-staged": "13.2.3",
    "prettier": "2.8.8",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "4.4.4"
  }
}
